    "q": "1.1.2",
    "request": "2.48.0",
    "underscore": "1.7.0"
  },
  "devDependencies": {
    "chai": "1.10.0",
    "chai-as-promised": "4.1.1",
    "jscs": "1.7.3",
    "jshint": "2.5.10",
