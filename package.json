    "require-subvert": "0.1.0",
    "sinon": "1.12.1",
    "sinon-chai": "2.6.0",
    "karma": "0.12.2",
    "karma-chrome-launcher": "0.1.2",
    "karma-mocha": "0.1.3",
    "karma-chai": "0.1.0",
    "karma-sinon": "1.0.3",
    "karma-phantomjs-launcher": "0.1.4",
    "karma-browserify": "1.0.0",
